  <rdf:Description rdf:about="http://www.semanticweb.org/tv/series/ontologies/Tv-Series-Ontology#defesa_à_medida_">
    <hasPilotYear rdf:datatype="http://www.w3.org/2001/XMLSchema#long">2011</hasPilotYear>
