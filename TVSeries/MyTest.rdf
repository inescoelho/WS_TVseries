  <rdf:Description rdf:about="http://www.semanticweb.org/tv/series/ontologies/Tv-Series-Ontology#gotham_">
    <rdf:type rdf:resource="http://www.semanticweb.org/tv/series/ontologies/Tv-Series-Ontology#Crime"/>
